<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculadoraServiceImplService WSDL service -->

  <service name="CalculadoraServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://calculadora"/>
      <parameter name="wsdlServiceElement" value="CalculadoraServiceImplService"/>
      <parameter name="schemaQualified" value="http://calculadora"/>
      <parameter name="wsdlServicePort" value="CalculadoraServiceImpl"/>
      <parameter name="className" value="calculadora.CalculadoraServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CalculadoraServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="dividir" qname="operNS:dividir" xmlns:operNS="http://calculadora" returnQName="retNS:dividirReturn" xmlns:retNS="http://calculadora" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="multiplicar" qname="operNS:multiplicar" xmlns:operNS="http://calculadora" returnQName="retNS:multiplicarReturn" xmlns:retNS="http://calculadora" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="somar" qname="operNS:somar" xmlns:operNS="http://calculadora" returnQName="retNS:somarReturn" xmlns:retNS="http://calculadora" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="subtrair" qname="operNS:subtrair" xmlns:operNS="http://calculadora" returnQName="retNS:subtrairReturn" xmlns:retNS="http://calculadora" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:a" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:b" xmlns:pns="http://calculadora" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="multiplicar somar subtrair dividir"/>

  </service>
</deployment>
